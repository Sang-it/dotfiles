nnoremap <leader><CR> :so ~/.config/nvim/init.vim<CR>
nnoremap <leader>= :vertical resize +5<CR>
nnoremap <leader>- :vertical resize -5<CR>
nnoremap <silent> Q <nop>
nmap <leader>q :q!<cr>
nmap <leader>w :w<cr>
inoremap <C-c> <esc>
nmap <C-h> <C-w>h
nmap <C-l> <C-w>l
nmap <C-j> <C-w>j
nmap <C-k> <C-w>k
no - $
no _ ^
