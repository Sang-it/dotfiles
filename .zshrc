# Path to your oh-my-zsh installation.
export ZSH="/Users/sangitmanandhar/.oh-my-zsh"

# Theme setup
ZSH_THEME="robbyrussell"	

#if [[ $TERM == xterm ]]; then
    #TERM=xterm-256color
